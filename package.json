{
  "name": "personal-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "npm run build:sanity:types",
    "build": "npm run prebuild && next build",
    "clean:sanity:types": "rm -f ./sanity/schema.json",
    "build:sanity:types": "npm run clean:sanity:types && npx sanity schema extract --path ./sanity/schema.json && npx sanity typegen generate",
    "start": "next start",
    "lint": "next lint",
    "test": "npx jest --passWithNoTests"
  },
  "dependencies": {
    "@portabletext/react": "^3.1.0",
    "@sanity/code-input": "^4.1.4",
    "@sanity/color-input": "^3.1.1",
    "@sanity/image-url": "^1.0.2",
    "@sanity/react-loader": "^1.10.3",
    "@sanity/vision": "^3.47.0",
    "next": "14.2.4",
    "next-sanity": "^9.3.10",
    "react": "^18",
    "react-dom": "^18",
    "react-player": "^2.16.0",
    "react-syntax-highlighter": "^15.5.0",
    "sanity": "^3.47.0",
    "styled-components": "^6.1.11"
  },
  "devDependencies": {
    "@sanity-typed/types": "^6.3.5",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-syntax-highlighter": "^15.5.13",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.5",
    "typescript": "^5"
  }
}
